{
  "extends": [
    "next",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["prettier",
    "@typescript-eslint/eslint-plugin"
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
}